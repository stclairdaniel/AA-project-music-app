<h1> <%= @album.title %> is an album by <%= Band.find(@album.band_id).name %></h1>

<h2>Tracks:</h2>
<ul>
<% @album.tracks.each do |track| %>
<li>
  <%= link_to "#{track.name}", track_url(track), method: :get  %>
</li>
<% end %>
<%= button_to "Add Track", new_album_track_url(@album), method: :get%>

<%= link_to "Edit Album", edit_album_url, method: :get %>
<%= link_to "Delete Album", album_url(@album), method: :delete %>
<%= link_to "Band Page", band_url(@album.band_id), method: :get %>
