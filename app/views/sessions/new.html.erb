<form action="<%= sessions_url %>" method="post">

  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label>
    E-mail:
    <input type="text" name="session[email]" value="<%= @user.email %>">
  </label>

  <label>
    Password:
    <input type="password" name="session[password]" value="<%= @user.password %>">
  </label>

  <input type="submit" value="Sign In">

</form>
